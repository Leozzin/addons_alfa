            <field name="list_price">0.0</field>
        </record>

        <record id="delivery_carrier_colissimo_wo_signature" model="delivery.carrier">
            <field name="name">Colissimo (w/o signature)</field>
            <field name="product_id" ref="a4o_delivery_colissimo.product_product_delivery_colissimo_wo_signature"/>
            <field name="coli_service_type">wo_signature</field>
            <field name="coli_country_origin_id" eval="ref('base.fr')"/>
