        <record id="specific_territory" model="res.country.group">
             <field name="name">Specific territory</field>
             <field name="country_ids" eval="[(6,0,[
                ref('base.es'), ref('base.it'), ref('base.de'),
                ref('base.uk'), ref('base.gr')])]"/>
       </record>
